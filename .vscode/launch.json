{
  "version": "1.0.0",
  "configurations": [
    {
      "name": "JS",
      "type": "node",
      "request": "launch",
      "program": "${file}",
      "cwd": "${workspaceFolder}",
      "outputCapture" : "std",
      "runtimeExecutable": "/Users/ringcrl/.nvm/versions/node/v14.17.3/bin/node"
    },
    {
      "name": "TS",
      "type": "node",
      "request": "launch",
      "runtimeArgs": [
        "-r",
        "ts-node/register"
      ],
      "args": [
        "${file}"
      ]
    },
    {
      "name": "Python",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    }
  ]
}
